
@model HomeViewModel



<!--body content wrap start-->
<div class="main">

    <!--hero section start-->
    <section class="hero-equal-height pt-165 pb-100 gradient-overly-right-light" style="background: url('../uploads/@Model.Mpartner.Photo')no-repeat center center / cover">
        <div class="container">
            <div class="row">
                <div class="col-md-7 col-lg-6">
                    <div class="hero-slider-content">
                        <span class="text-uppercase">@Model.Mpartner.About</span>
                        <h1>@Model.Mpartner.Title</h1>
                        <p class="lead">@Model.Mpartner.Desc </p>

                        <div class="action-btns mt-3">
                            <a href="#" class="btn secondary-solid-btn">@Model.Mpartner.Button</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--hero section end-->
    

    <partial name="_promoblock" model="@Model.Promos" />



    <!--about us section start-->
    <section class="about-us-section pb-100">
        <div class="container">
            <div class="row justify-content-between align-items-center">
                <div class="col-md-12 col-lg-6">
                    <div class="video-promo-content mb-md-4 mb-lg-0">
                        <h2>@Model.BusinesSolution.Title</h2>
                        <p class="lead">
                           @Model.BusinesSolution.Text
                        </p>
                        <ul class="list-unstyled tech-feature-list">
                            @foreach (var item in Model.BusinesSolutionItem)
                            {
                                <li class="py-1"><span class="ti-control-forward mr-2 color-secondary"></span><strong>@item.Title</strong> @item.Desc</li>

                            }

                           
                        </ul>
                        <div class="action-btns mt-4">
                            <a href="#" class="btn primary-solid-btn mr-2">View Our Services</a>
                            <a href="#" class="btn outline-btn">Contact Us</a>
                        </div>
                    </div>
                </div>
                <div class="col-md-12 col-lg-6">
                    <div class="image-wrap">
                        <img src="~/uploads/@Model.BusinesSolution.Photo" alt="video" class="img-fluid rounded">
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--about us section end-->
   
    <partial name="_services" model="@Model.Services"/>


    <!--promo section start-->
    <section class="promo-section gradient-bg ptb-100">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-md-9 col-lg-8">
                    <div class="app-business-content text-center text-white">
                        <h2 class="text-white">@Model.Download.Title</h2>
                        <p class="lead">
                           @Model.Download.Desc
                        </p>

                        <div class="action-btns mt-5">
                            <ul class="list-inline app-download-list">

                                @foreach (var item in Model.ButtonDownloads)
                                {<li class="list-inline-item">
                                  <a href="#" class="d-flex align-items-center border rounded">
                                          <span class="@item.Icon icon-sm mr-3"></span>
                                         <div class="download-text text-left">
                                            <span>Download form</span>
                                                  <h5 class="mb-0">@item.Name</h5>
                                           </div>
                                            </a>
                                   </li>

                                }
                                
                               
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--promo section end-->
    
    <partial name="_checkourwork" model="@Model.Projects" />


    <!--blog section start-->
    <section class="our-blog-section ptb-100 gray-light-bg">
        <div class="container">
            <div class="row">
                <div class="col-md-8">
                    <div class="section-heading mb-5">
                        <h2>Our Latest News</h2>
                        <p class="lead">
                            Enthusiastically drive revolutionary opportunities before emerging leadership. Distinctively
                            transform tactical methods of empowerment via resource.
                        </p>
                    </div>
                </div>
            </div>
            <div class="row justify-content-center">
                @foreach (var item in Model.News)
                {<div class="col-md-6 col-lg-4">
                        <div class="single-blog-card card border-0 shadow-sm">
                            <div class="blog-img">
                                <a href="#"><span class="category position-absolute">@item.Position</span></a>
                                <a href="#"><img src="~/uploads/@item.Photo" class="card-img-top position-relative img-fluid" alt="blog"></a>
                            </div>
                            <div class="card-body">
                                <h3 class="h5 mb-2 card-title"><a href="#">@item.Title</a></h3>
                                <p class="card-text">@item.Text</p>
                            </div>
                            <div class="card-footer border-0 d-flex align-items-center justify-content-between">
                                <div class="author-meta d-flex align-items-center">
                                    <span class="fa fa-user mr-2 p-3 bg-white rounded-circle border"></span>
                                    <div class="author-content">
                                        <a href="#" class="d-block">ThemeTags</a>
                                        <small>@item.Date</small>
                                    </div>
                                </div>
                                <div class="author-like">
                                    <a href="#"><span class="fa fa-share-alt"></span> 50</a>
                                </div>
                            </div>
                        </div>
                    </div>

                }

                
            </div>
        </div>
    </section>
    <!--blog section end-->
   


    <partial name="_clientsay" model="@Model.Clients" />


    
    <partial name="_meetteam" model="@Model.Members" />


   <partial name="_calltoaction" />

</div>
<!--body content wrap end-->